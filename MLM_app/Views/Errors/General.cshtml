
@{
    ViewBag.Title = "General";
    Layout = "~/Views/Shared/_LayoutMLM.cshtml";
}

<h2>General</h2>

<div>
    <h2>An unexpected error has occurred</h2>
    @if (Model != null)
    {
        <p>
            @Model.Exception.GetType().Name

            thrown in @Model.ControllerName @Model.ActionName
        </p>
    }
</div>



